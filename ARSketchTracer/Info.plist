    <key>NSPhotoLibraryUsageDescription</key>
    <string>Allow access to choose a tracing image.</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>Allow saving snapshots to your library.</string>
    <key>UIAppFonts</key>
    <array>
        <string>Merriweather-VariableFont_opsz,wdth,wght.ttf</string>
        <string>Merriweather-Italic-VariableFont_opsz,wdth,wght.ttf</string>
    </array>
